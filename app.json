{
  "expo": {
    "name": "ONL HRIS",
    "slug": "onl-hris",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/HRIS.png",
    "splash": {
      "image": "./assets/splashNew.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onglaiid.employee"
    },
    "android": {
      "versionCode": 12,
      "googleServicesFile": "./google-services.json",
      "package": "com.onglaiid.employee",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/AIcon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/HRIS.png"
    }
  }
}
