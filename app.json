{
  "expo": {
    "name": "ONL HRIS",
    "slug": "onl-hris-onglai",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/HRIS-01.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "sounds" : ["./assets/peringatan.wav"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splashNew.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onglainextlab.app"
    },
    "android": {
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "package": "com.onglainextlab.app",
      "useNextNotificationsApi": true,
      "permissions": ["RECEIVE_BOOT_COMPLETED"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/HRIS-02.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/HRIS.png"
    }
  }
}
